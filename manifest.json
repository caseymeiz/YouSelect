{
    "manifest_version":2,

    "name": "YouSelect",
    "description": "Video navigation with tab",
    "version": "0.1",

    "content_scripts": [
        {
        "matches": [
            "http://www.youtube.com/*",
             "https://www.youtube.com/*"
        ],
        "js": ["jquery.js",
            "youSelect.js"
        ],
        "css":["style.css"]
        }
    ],

    "permissions": [
          "tabs"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }

}
